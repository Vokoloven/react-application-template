{
  "name": "react-app-template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack server --config webpack.dev.js",
    "build": "webpack --env ENV_FILE=.env --config webpack.prod.js",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint --fix src",
    "format": "prettier --write \"**/*.{js,ts,html,css,json}\"",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint",
    "*.{js,ts,tsx,css,scss,json}": "prettier --write"
  },
  "keywords": [],
  "author": "Ruslan Volovenko",
  "license": "ISC",
  "description": "React Application Template",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.20.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.8.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.15.0",
    "html-webpack-plugin": "^5.6.3",
    "interpolate-html-plugin": "^4.0.0",
    "lint-staged": "^15.4.3",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.5.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
